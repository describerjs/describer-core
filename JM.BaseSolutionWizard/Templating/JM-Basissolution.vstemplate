      </SolutionFolder>

      <SolutionFolder Name="Web">

        <ProjectTemplateLink ProjectName="$projectname$.Web">JM.Web\JM.Web.vstemplate</ProjectTemplateLink>

      </SolutionFolder>

    </ProjectCollection>

  </TemplateContent>
<WizardExtension>
    <Assembly>
      JM.BaseSolutionWizard, Version=1.0.0.1, Culture=neutral, PublicKeyToken=79350b940fb8868c
    </Assembly>
    <FullClassName>JM.BaseSolutionWizard.InstallWizard</FullClassName>
  </WizardExtension>
