    <Name>JM.BaseSolution</Name>

   <Description>DESCRIPTION</Description>

    <ProjectType>CSharp</ProjectType>

  </TemplateData>

  <TemplateContent>

    <ProjectCollection>

      <SolutionFolder Name="Modules">

        <ProjectTemplateLink ProjectName="JM.Business">JM.Business\JM.Business.vstemplate</ProjectTemplateLink>

        <ProjectTemplateLink ProjectName="JM.Business.Kontakt">JM.Business.Kontakt\JM.Business.Kontakt.vstemplate</ProjectTemplateLink>

